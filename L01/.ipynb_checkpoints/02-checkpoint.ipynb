   "source": [
    "Let \n",
    "$\n",
    "\\rho := \\text{density}\n",
    "$, \n",
    "\n",
    "Then,\n",
    "$\n",
    "\\text{mass} = \\rho * \\text{volume}\n",
    "$,\n",
    "\n",
    "$\n",
    "\\implies dM_r (r) = \\rho(r) \\cdot 4 \\pi r^2 dr\n",
    "$\n"
   ]
  },
  {
